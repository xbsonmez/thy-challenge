{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "Components/*": [
        "src/components/*"
      ],
      "Pages/*": [
        "src/pages/*"
      ],
      "Styles/*": [
        "src/styles/*"
      ],
      "Public/*": [
        "public/*"
      ],
      "Utils/*": [
        "src/utils/*"
      ],
      "Configs/*": [
        "configs/*"
      ],
      "Store/*": [
        "src/redux/*"
      ],
      "Context/*": [
        "src/context/*"
      ],
      "Constants/*": [
        "src/constants/*"
      ],
      "Tools/*": [
        "src/tools/*"
      ]
    }
  }
}